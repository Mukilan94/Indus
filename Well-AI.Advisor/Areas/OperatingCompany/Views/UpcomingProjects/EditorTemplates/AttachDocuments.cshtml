@model WellAI.Advisor.Model.ServiceCompany.Models.AddProjectAttachmentViewModel
<script>
    $(function () {
        $("data").kendoValidator();
    });
</script>
<style type="text/css">
    .k-window-content,
    .k-prompt-container {
        overflow: hidden !important;
    }
</style>
@using (Html.BeginForm(FormMethod.Post, new { enctype = "multipart/form-data", @id="data" ,@name="data"}))
{
    <div class="row">
        @Html.HiddenFor(m => m.ProposalId)
        @Html.HiddenFor(m => m.ProjectCode)
        @Html.HiddenFor(m => m.ProjectId, new { @class = "setProjectId" })
        <div class="col-sm-12">
            @(Html.Kendo().Upload()
                            .Name("files")
                            .HtmlAttributes(new { aria_label = "files" }))
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="row">
                <div class="col-sm-10"></div>
                <div class="col-sm-2">
                    <input type="button" onclick="uploadprojectFile()" class="k-button k-button-icontext k-primary k-grid-update" value="Upload file" />
                </div>
            </div>
        </div>
    </div>
}