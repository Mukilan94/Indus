                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4" id="CityBlock" style="display:block">
                                <div class="k-edit-field">
                                    @Html.Kendo().TextBox().Name("city").Placeholder("City").HtmlAttributes(new { @maxlength = "50", required = "required", validationmessage = "City is required" })
                                </div>
                            </div>
                            <div class="col-md-4" id="StateBlock" style="display:block">
                                <div class="k-edit-field ">
                                    @Html.Kendo().TextBox().Name("state").Placeholder("State").HtmlAttributes(new { @maxlength = "50", required = "required", validationmessage = "State is required" })
