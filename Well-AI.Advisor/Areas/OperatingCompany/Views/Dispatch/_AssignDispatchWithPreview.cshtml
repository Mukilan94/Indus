                            <div class="col-md-3" id="LongitudeBlock" style="display:block">
                                <div class="k-edit-field">

                                    @Html.Kendo().TextBox().Name("longitude").Placeholder("Longitude").HtmlAttributes(new { required = "required", validationmessage = "Longitude is required" })
                                </div>
                            </div>
                        </div>
                        <div id="message">

                        </div>
                    </div>
                </form>
